<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Hunter</title>
    <environment include="Development">
        <link rel="stylesheet" href="~/Libraries/bootstrap/3.3.7/css/bootstrap.css" />
		<link rel="stylesheet" href="~/Libraries/font-awesome/4.7.0/css/font-awesome.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/Libraries/bootstrap/3.3.7/css/bootstrap.min.css"  />
		<link rel="stylesheet" href="~/Libraries/font-awesome/4.7.0/css/font-awesome.min.css" />
    </environment>
    <style>
        html, body { width: 100%; height: 100%; font-size: 12px; overflow: auto; }
		.table > tbody > tr > th { text-align: center; vertical-align: middle; }
		.table > tbody > tr > td { text-align: center; vertical-align: middle; }
        .position-absolute { position: absolute; } 
        .position-absolute.layout { top: 0px; left: 0px; right: 0px; bottom: 0px; overflow: auto; }
    </style>
    @this.RenderSection("Styles", false)
</head>
<body>
    @this.RenderBody()
    <environment include="Development">
        <script src="~/Libraries/jquery/jquery-3.2.1.js"></script>
        <script src="~/Libraries/bootstrap/3.3.7/js/bootstrap.js"></script>
        <script src="~/Scripts/jQuery.extension.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/Libraries/jQuery/jquery-3.2.1.min.js"></script>
        <script src="~/Libraries/bootstrap/3.3.7/js/bootstrap.min.js"></script>
        <script src="~/Scripts/jQuery.extension.js"></script>
    </environment>
    @this.RenderSection("Scripts", false)
</body>
</html>
